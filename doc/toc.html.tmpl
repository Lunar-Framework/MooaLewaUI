<div id="sidetoggle">
  <div>
    <div class="sidetoc">
      {{^_disableSideFilter}}
      <div class="sidefilter">
        <form class="toc-filter">
          <span class="glyphicon glyphicon-filter filter-icon"></span>
          <span class="glyphicon glyphicon-remove clear-icon" id="toc_filter_clear"></span>
          <input type="text" id="toc_filter_input" placeholder="Type here to filter..." onkeypress="if(event.keyCode==13) {return false;}">
        </form>
      </div>
      {{/_disableSideFilter}}
      <div class="toc" id="toc">
        {{^leaf}}
          {{>partials/li}}
        {{/leaf}}
      </div>
    </div>
    <div class="sidetoc-separator">
    </div>
  </div>
</div>
